{
  "name": "@digita-ai/nde-erfgoed-manage",
  "version": "0.0.1",
  "description": "Collection of Semantic Components for the NDE-project.",
  "author": "Wouter Janssens <wouter@digita.ai>",
  "main": "./dist/nde-erfgoed-manage.umd.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/nde-erfgoed-manage.es.js",
  "exports": {
    ".": {
      "import": "./dist/nde-erfgoed-manage.es.js",
      "require": "./dist/nde-erfgoed-manage.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start:watch": "rm -rf ./dist && npm run build && vite --config vite.serve.config.ts",
    "build": "rm -rf ./dist && vite build --config vite.build.config.ts && tsc",
    "build:dev": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --passWithNoTests",
    "test:ci": "npm run test -- --runInBand"
  },
  "dependencies": {
    "@digita-ai/nde-erfgoed-components": "0.0.1",
    "@digita-ai/nde-erfgoed-core": "0.0.1",
    "@digita-ai/nde-erfgoed-theme": "0.0.1",
    "@digita-ai/semcom-core": "0.3.4",
    "@inrupt/solid-client": "^1.6.1",
    "@inrupt/solid-client-authn-browser": "^1.8.0",
    "crypto-browserify": "^3.12.0",
    "lit-element": "2.4.0",
    "rx-lit": "1.0.1",
    "rxjs": "^6.6.7",
    "xstate": "^4.17.1",
    "xstate-codegen": "0.1.0-next.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "esbuild": "^0.11.6",
    "eslint": "^7.16.0",
    "jest": "26.6.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "^4.1.3",
    "vite": "2.2.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digita-ai/nde-erfgoedinstellingen.git",
    "directory": "packages/nde-erfgoed-manage"
  },
  "bugs": {
    "url": "https://github.com/digita-ai/nde-erfgoedinstellingen/issues"
  }
}
